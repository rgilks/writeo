{
  "name": "writeo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "nuke": "rm -rf package-lock.json node_modules .next .next-validation  && npm install && npm run dev",
    "deps": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u && npm run nuke",
    "test:e2e": "playwright test",
    "verify": "prettier --write . && next lint --fix --no-cache && npx tsc --noEmit",
    "check": "npm run verify && npm run test:e2e"
  },
  "dependencies": {
    "@aws-amplify/adapter-nextjs": "^1.6.3",
    "@aws-amplify/ui-react": "^6.11.2",
    "aws-amplify": "^6.15.0",
    "immer": "^10.1.1",
    "next": "15.3.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^3.25.61",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.16.1",
    "@aws-amplify/backend-cli": "^1.7.2",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.53.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "aws-cdk": "^2.1018.0",
    "aws-cdk-lib": "^2.200.1",
    "constructs": "^10.4.2",
    "esbuild": "^0.25.5",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.5.3",
    "tailwindcss": "^4",
    "tsx": "^4.20.1",
    "typescript": "^5.8.3"
  }
}
